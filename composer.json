{
    "name": "dive-be/nova-translation-editor",
    "description": "A Laravel Nova tool for editing your translation strings.",
    "keywords": [
        "dive",
        "translations",
        "i18n",
        "localization",
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/dive-be/nova-translation-editor",
    "license": "MIT",
    "authors": [
        {
            "name": "Michiel Vancoillie",
            "email": "michiel@dive.be",
            "homepage": "https://dive.be",
            "role": "Developer"
        },
        {
            "name": "Muhammed Sari",
            "email": "muhammed@dive.be",
            "homepage": "https://dive.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0",
        "laravel/nova": "^3.31"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Dive\\NovaTranslationEditor\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.dist.php --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dive\\NovaTranslationEditor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
